{
  "name": "Conky",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Word of the Day",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
	},
   
  "permissions": [
    "notifications"
  ],
  
  "content_scripts":[
	{
		"matches": ["http://*/*"],
		"js": ["popup.js"],
		"background": {
			"scipts": [ "popup.js"]
		}
	}
  ]
}